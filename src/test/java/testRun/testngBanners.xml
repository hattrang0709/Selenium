<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="testng">
	 <listeners>
  		<listener class-name="common.Listeners"></listener>
  	 </listeners>
  <test name="testRun" >
	  <parameter name="browser" value="ie"/>
	  <parameter name="timeout" value="150"/>
	  <parameter name="url" value="http://192.168.188.150/joomla/administrator/"/>
		<classes>
			<class name="testCase.TC_JOOMLA_BANNERS_001_VerifyThatUserCanCreateNewBanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_002_VerifyThatUserCanEditABanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_003_VerifyThatUserCanCreateANewBannerWithUnpublishedStatus" />
			<class name="testCase.TC_JOOMLA_BANNERS_004_VerifyThatUserCanUnpublishABanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_005_VerifyThatUserCanArchiveABanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_006_VerifyThatUserCanSendABannerToTrash" />
			<class name="testCase.TC_JOOMLA_BANNERS_007_VerifyThatUserCanBrowseBannerHelpPage" />
			<class name="testCase.TC_JOOMLA_BANNERS_008_VerifyThatUserCanSearchABannerByUsingFilterTextbox" />
			<class name="testCase.TC_JOOMLA_BANNERS_009_VerifyThatUserCanSearchABannerByUsingFilterDropdownLists" />
			<class name="testCase.TC_JOOMLA_BANNERS_010_VerifyThatUserCanCheckInABanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_011_VerifyThatUserCanCreateManyBannersByUsingSaveNewButton" />
			<class name="testCase.TC_JOOMLA_BANNERS_012_VerifyThatUserCanBrowseNewBannerHelpPageInNewBannerPage" />
			<class name="testCase.TC_JOOMLA_BANNERS_013_VerifyThatUserCanCreateANewBannerByUsingSaveAsCopyButton" />
			<class name="testCase.TC_JOOMLA_BANNERS_014_VerifyThatUserCannotCreateANewBannerWithoutEnteringTheNameOfTheBanner" />
			<class name="testCase.TC_JOOMLA_BANNERS_015_VerifyThatUserCanChangeTheQuantityOfItemsDisplayedInBannerTable" />
			<class name="testCase.TC_JOOMLA_BANNERS_016_VerifyThatUserCanSortItemsDisplayedInBannerTableByID" />
			<class name="testCase.TC_JOOMLA_BANNERS_017_VerifyThatUserCanAccessBannerClientsPageWhileBrowsingBannerPage" />
		</classes>
	</test>
</suite>

